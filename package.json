{
  "name": "auction-service",
  "version": "1.0.0",
  "description": "Template for Serverless Framework projects",
  "author": "Adam Colpitts <acolpitts@intercap.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "build": "babel src --out-dir dist",
    "validate": "npm run lint && npm run format && npm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.21.4",
    "@serverless/eslint-config": "^5.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "serverless-bundle": "^6.0.0",
    "serverless-offline": "^12.0.4"
  },
  "dependencies": {
    "aws-sdk": "^2.639.0"
  }
}
